{
	"name": "Night Mind deepPurple",
	"type": "dark",
	"include": "./defaults_purple.json",
	"semanticHighlighting": false,
	"semanticTokenColors": {
		"variable.parameter": { "foreground": "#e0b96b", "fontStyle": "italic" },
		"variable.readonly": { "foreground": "#e0a93d", "fontStyle": "normal" }
	},

	"tokenColors": [
		{
			"name": "Global settings",
			"settings": {
				"background": "#1B1B1B"
			}
		},
		{
			"name": "normalize font style of certain components",
			"scope": [
				"meta.property-list.css meta.property-value.css variable.other.less",
				"meta.property-list.scss variable.scss",
				"meta.property-list.sass variable.sass",
				"meta.brace",
				"keyword.operator.operator",
				"keyword.operator.or.regexp",
				"keyword.operator.expression.in",
				"keyword.operator.relational",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.type",
				"keyword.operator",
				"keyword",
				"punctuation.definition.string",
				"punctuation",
				"storage.type",
				"source.css",
				"string.quoted"
			],
			"settings": {
				"fontStyle": "normal"
			}
		},

		{
			"name": "Changed",
			"scope": [
				"markup.changed",
				"meta.diff.header.git",
				"meta.diff.header.from-file",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "#8694e8"
			}
		},

		{
			"name": "Deleted",
			"scope": "markup.deleted.diff",
			"settings": {
				"foreground": "#F05050"
			}
		},

		{
			"name": "Inserted",
			"scope": "markup.inserted.diff",
			"settings": {
				"foreground": "#A1D884"
			}
		},

		{
			"name": "Language Variable",
			"scope": "variable.language",
			"settings": {
				"foreground": "#00B1FF"
			}
		},

		{
			"name": "Constant Character Escape",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#AAE682"
			}
		},

		{
			"name": "Meta Tag",
			"scope": ["punctuation.definition.tag", "meta.tag"],
			"settings": {
				"foreground": "#CECECE"
			}
		},

		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#e7ca8d"
			}
		},

		{
			"name": "Support Variable Property",
			"scope": "support.variable.property",
			"settings": {
				"foreground": "#e0b96b"
			}
		},

		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F05050",
				"background": "#392020"
			}
		},

		{
			"name": "RegExp String",
			"scope": [
				"string.regexp",
				"string.regexp keyword.other",
				"string.regexp punctuation.definition.string",
				"constant.character.escape.backslash.regexp",
				"constant.other.character-class.set.regexp",
				"constant.character.control.regexp"
			],
			"settings": {
				"foreground": "#DBD4BA"
			}
		},

		{
			"name": "Constant Other Color",
			"scope": ["support.constant.color", "constant.other.color"],
			"settings": {
				"foreground": "#CECECE"
			}
		},

		{
			"name": "Italics",
			"scope": "italic",
			"settings": {
				"foreground": "",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Bold",
			"scope": "bold",
			"settings": {
				"foreground": "#AAE682",
				"fontStyle": "bold"
			}
		},

		{
			"name": "Quote",
			"scope": "quote",
			"settings": {
				"foreground": "#AAE682",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#F05050",
				"foreground": "#FFFFFF"
			}
		},

		{
			"name": "Library class/type",
			"scope": ["support.constant.json", "support.type", "support.class"],
			"settings": {
				"foreground": "#d2d1ff",
				"fontStyle": "normal"
