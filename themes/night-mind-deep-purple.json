				"variable.other.instance",
				"variable.readwrite.instance",
				"variable.other.readwrite.instance",
				"variable.other.property"
			],
			"settings": {
				"foreground": "#e0b96b"
			}
		},

		{
			"name": "Support Constant Math",
			"scope": ["support.constant.math"],
			"settings": {
				"foreground": "#d2d1ff",
				"fontStyle": "normal"
			}
		},

		{
			"name": "Storage type",
