			"settings": {
				"foreground": "#9494ff"
			}
		},

		{
			"name": "Entity Name Tag Custom",
			"scope": "entity.name.tag.custom",
			"settings": {
				"foreground": "#b994ff"
			}
		},

		{
			"name": "Meta Delimiter Period",
			"scope": "meta.delimiter.period",
			"settings": {
				"foreground": "#b994ff"
			}
		},

		{
			"name": "Meta Selector",
			"scope": "meta.selector",
			"settings": {
				"foreground": "#b994ff"
			}
		},

		{
			"name": "Doctypes",
